{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "text/css"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "analytics": true,
  "speedInsights": {
    "enabled": true
  }
}
